CREATE TABLE IF NOT EXISTS Reminder(
    id TEXT NOT NULL PRIMARY KEY,
    game TEXT,
    runner TEXT,
    startTime TEXT,
    time TEXT,
    info TEXT
);

getReminders:
SELECT * FROM Reminder;

addReminder:
INSERT OR REPLACE INTO Reminder(id, game, runner, startTime, time, info) VALUES (?,?,?,?,?,?);

deleteReminder:
DELETE FROM Reminder WHERE id = ?;